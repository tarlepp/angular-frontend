    "@ngrx/effects": "11.0.1",
    "@ngrx/entity": "11.0.1",
    "@ngrx/router-store": "11.0.1",
    "@ngrx/store": "11.0.1",
    "@ngrx/store-devtools": "11.0.1",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "ngrx-store-localstorage": "11.0.0",

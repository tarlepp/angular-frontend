    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "protractor": "7.0.0",
    "snyk": "1.680.0",
    "stylelint": "13.13.1",
    "stylelint-config-angular-material": "1.0.0",
    "stylelint-config-sass-guidelines": "8.0.0",
    "stylelint-scss": "3.20.1",
